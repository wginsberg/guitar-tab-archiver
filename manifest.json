{
  "manifest_version": 3,
  "name": "Tab Archiver",
  "version": "1.0.1",
  "description": "Archives guitar tabs on ultimate-guitar.com",
  "content_scripts": [
    {
      "matches": ["*://*.ultimate-guitar.com/*"],
      "js": ["index.js"]
    }
  ],
  "permissions": ["storage", "identity"],
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "key": "",
  "oauth2": {
    "client_id": "993606245930-3fgtksjef3n23nc1oofllueavd6nq8eb.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/drive.file"]
  }
}
