{
  "manifest_version": 3,
  "name": "Tab Archiver",
  "version": "0.1.0",
  "description": "Archives guitar chords and tabs from ultimate-guitar.com",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.ultimate-guitar.com/*"],
      "js": ["ug.js"]
    }
  ],
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
